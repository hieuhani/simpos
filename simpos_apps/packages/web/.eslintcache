onfig.modulePosHr ? 'button' : 'div'}\n            direction=\"row\"\n            alignItems=\"center\"\n            spacing={2}\n            onClick={changeCashier}\n          >\n            {cashier && (\n              <Box textAlign=\"left\">\n                <Text fontSize=\"sm\">Thu ngn</Text>\n                <Heading size=\"sm\">{cashier.name}</Heading>\n              </Box>\n            )}\n\n            {posConfig.modulePosHr && <IconSync size=\"1.5rem\" />}\n          </Stack>\n        </Stack>\n      </Flex>\n      <Modal\n        isOpen={showChangeCashiser}\n        onClose={onCloseChangeCashiser}\n        scrollBehavior=\"inside\"\n      >\n        <ModalOverlay />\n        <ModalContent>\n          <ModalHeader>Chn thu ngn</ModalHeader>\n          <ModalCloseButton />\n          <ModalBody>\n            <Stack spacing={2}>\n              {employees.map((employee) => (\n                <EmployeeRow\n                  key={employee.id}\n                  employee={employee}\n                  checked={selectEmployee?.id === employee.id}\n                  onClick={() => setSelectEmployee(employee)}\n                />\n              ))}\n            </Stack>\n          </ModalBody>\n          <ModalFooter>\n            <Button\n              colorScheme=\"red\"\n              disabled={!selectEmployee}\n              onClick={selectCashier}\n            >\n              Chn\n            </Button>\n          </ModalFooter>\n        </ModalContent>\n      </Modal>\n    </>\n  );\n};\n","/Users/hieuhani/code/simpos/simpos_apps/packages/web/src/components/DrawerNavigation/SubMenuItem.tsx",[],"/Users/hieuhani/code/simpos/simpos_apps/packages/web/src/apps/pos/components/OrderStatus/index.ts",[],"/Users/hieuhani/code/simpos/simpos_apps/packages/web/src/components/DrawerNavigation/MenuItem.tsx",[],"/Users/hieuhani/code/simpos/simpos_apps/packages/web/src/apps/pos/components/MakePayment/index.ts",[],"/Users/hieuhani/code/simpos/simpos_apps/packages/web/src/hooks/